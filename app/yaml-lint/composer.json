{
    "name": "app/yaml-lint",
    "type": "library",
    "version": "0.0.0",
    "authors": [
        {
            "name": "Mike Street",
            "email": "mike@liquidlight.co.uk"
        }
    ],
    "require": {
        "app/linter": "0.0.0",
        "symfony/yaml": "6.4.3"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./../*",
            "options": {
                "symlink": false
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "LiquidLight\\YamlLint\\": "src"
        }
    }
}
