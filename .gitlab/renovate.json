{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"rangeStrategy": "pin",
	"ignoreDeps": [
		"phpstan/phpstan"
	],
	"major": {
		"enabled": true
	},
	"packageRules": [
		{
			"matchUpdateTypes": ["major"],
			"addLabels": ["Action: Review Required", "Review: With Developer"]
		}
	]
}
