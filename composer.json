{
	"name": "liquidlight/lint",
	"description": "Liquid Light Linter",
	"type": "project",
	"version": "dev-main",
	"authors": [
		{
			"name": "Mike Street",
			"email": "mike@liquidlight.co.uk"
		}
	],
	"require": {
		"ergebnis/composer-normalize": "^2.28",
		"friendsofphp/php-cs-fixer": "^3.0",
		"helmich/typo3-typoscript-lint": "^3.0",
		"j13k/yaml-lint": "^1.1",
		"phpstan/phpstan": "^1.5",
		"seld/jsonlint": "^1.8",
		"symfony/console": "^5.2",
		"symfony/finder": "^5.3",
		"symfony/translation": "^5.4",
		"symfony/yaml": "^5.2"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src"
		}
	},
	"config": {
		"allow-plugins": {
			"bamarni/composer-bin-plugin": true,
			"ergebnis/composer-normalize": true
		}
	}
}
